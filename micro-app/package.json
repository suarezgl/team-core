    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle": "ng build --project micro-app --prod --output-hashing none  --configuration=production  --single-bundle true",
